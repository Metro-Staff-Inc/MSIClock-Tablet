// Cloudflare R2 Credentials Configuration
//
// INSTRUCTIONS:
// 1. Copy this file to 'r2_credentials.dart' (same directory)
// 2. Fill in your actual Cloudflare R2 credentials
// 3. The r2_credentials.dart file is gitignored and will NOT be committed
//
// To get your R2 credentials:
// 1. Log in to Cloudflare Dashboard
// 2. Go to R2 Object Storage
// 3. Create a bucket (if you haven't already)
// 4. Go to "Manage R2 API Tokens"
// 5. Create an API token with read/write permissions for your bucket
// 6. Note your Account ID from the R2 overview page

class R2Credentials {
  // Your Cloudflare Account ID (found in R2 overview)
  static const String accountId = 'YOUR_ACCOUNT_ID_HERE';
  
  // Your R2 bucket name (e.g., 'msi-clock-logs')
  static const String bucketName = 'YOUR_BUCKET_NAME_HERE';
  
  // Your R2 API Access Key ID
  static const String accessKeyId = 'YOUR_ACCESS_KEY_ID_HERE';
  
  // Your R2 API Secret Access Key
  static const String secretAccessKey = 'YOUR_SECRET_ACCESS_KEY_HERE';
}
